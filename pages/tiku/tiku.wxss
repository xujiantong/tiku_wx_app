/* pages/tiku/tiku.wxss */
.tiku{
  width: 100%;
  height: 100%;
  position: relative;
  background-color: #ffffff;
}
.no_data{
  display: flex;
  flex-flow: column nowrap;
  justify-content: center;
  align-items: center;
  height: 100%;
}
.no_data image{
  width: 500rpx;
  height: 260rpx;
}
.no_data text {
  width: 100%;
  height: 50rpx;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
  margin-top: 20rpx;
  color: #ccc;
  font-size: 26rpx;
}